<div class="control-group">
  <label for="name" class="control-label">name</label>
  <div class="controls">
    <%- contentTag('input', product.name, {type:'text', class:'span6', name:'name'}) %>
  </div>
</div>
<div class="control-group">
  <label for="quantity" class="control-label">quantity</label>
  <div class="controls">
    <%- contentTag('input', product.quantity, {type:'number', class:'span2', name:'quantity'}) %>
  </div>
</div>
<div class="control-group">
  <label for="expiryDate" class="control-label">expiryDate</label>
  <div class="controls">
    <%- contentTag('input', product.expiryDate, {type:'date', class:'span2', name:'expiryDate'}) %>
  </div>
</div>

